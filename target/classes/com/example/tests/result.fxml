<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" style="-fx-background-color: #f0ebf8;" stylesheets="@style.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tests.client.controller.ResultController">
    <children>
        <VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" prefWidth="400.0" spacing="30.0" style="-fx-background-color: white;" styleClass="result-form">
            <children>
                <Label alignment="BASELINE_CENTER" maxWidth="1.7976931348623157E308" text="TEST RESULTS">
                    <font>
                        <Font name="System Bold" size="24.0" />
                    </font>
                </Label>
                <VBox>
                    <children>
                        <Label alignment="BASELINE_CENTER" maxWidth="1.7976931348623157E308" text="Score:">
                            <font>
                                <Font size="18.0" />
                            </font>
                        </Label>
                        <Label fx:id="result" alignment="BASELINE_CENTER" maxWidth="1.7976931348623157E308" text="9/10">
                            <font>
                                <Font size="18.0" />
                            </font>
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                        </Label>
                    </children>
                </VBox>
                <Button mnemonicParsing="false" onAction="#goToHomePage" styleClass="create-button" text="Home">
                    <VBox.margin>
                        <Insets top="70.0" />
                    </VBox.margin>
                </Button>
            </children>
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding>
        </VBox>
    </children>
    <padding>
        <Insets bottom="50.0" left="50.0" right="50.0" />
    </padding>
</VBox>
